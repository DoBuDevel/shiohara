				<xs:element name="editorStyle" type="xs:string" minOccurs="0"/>
				<xs:element name="acl" type="CT_acl" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="AG_vcmAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="group">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ===========================================================================
  = Name:  group
  = Desc:  Group assignments.
  ===========================================================================
  			]]></xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="contents">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ===========================================================================
  = Name:  contents
  = Desc:  For static files, workflows, and program tasks, this specifies
  =        the name of the ZIP entry in the package file containing the
  =        object's body.
  ===========================================================================
  			]]></xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="entry" type="xs:string" use="optional"/>
			<xs:attribute name="file" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="AG_appSvcsAttributes">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  AG_appSvcsAttributes
  - Desc:  Defines common AppSvcs attribs all managed/unmanaged objects
  -        contain.
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:attribute name="mgmtId" type="AT_id" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="AG_moAttributes">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  AG_moAttributes
  - Desc:  Defines common attributes all managed objects contain.
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:attribute name="vcmId" type="AT_id" use="optional"/>
		<xs:attribute name="vcmStatus" type="xs:string" use="optional"/>
		<xs:attribute name="vcmName" type="xs:string" use="optional"/>
		<xs:attribute name="vcmLogicalPath" type="xs:string" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="AG_vcmAttributes">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  AG_vcmAttributes
  - Desc:  Defines common VCM attribs all CMSObjects contain.
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:attribute name="vcmId" type="AT_id" use="optional"/>
	</xs:attributeGroup>
	<xs:simpleType name="AT_id">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  AT_id
  - Desc:  Type for attributes whose values are ID's (VCM or AppSvcs).
  -        The <getId> element is not allowed here.
  ---------------------------------------------------------------------------
   			]]></xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value=".{40}|.{0}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CT_acl">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_acl
  - Desc:  Defines an access control list (ACL).
  - Exam:  (see "capability" below for its content model)
  -        <acl>
  -          <entry name="aaa" type="group">
  -            <grants>
  -              <capability name="bbb"/>
  -              <capability name="ccc"/>
  -            </grants>
  -            <revokes>
  -              <capability name="ddd"/>
  -            </revokes>
  -          </entry>
  -          <entry name="eee" type="user">
  -            ...
  -          </entry>
  -        </acl>
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="entry" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="grants" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="capability" type="CT_capability" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="revokes" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="capability" type="CT_capability" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="name" type="xs:string" use="required"/>
					<xs:attribute name="type" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:NMTOKEN">
								<xs:enumeration value="user"/>
								<xs:enumeration value="group"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CT_capability">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_capability
  - Desc:  Defines a Capability.
  - Exam:  <capability application="VCM" name="MODIFY"/>
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:attribute name="application" type="xs:string" use="required"/>
		<xs:attribute name="name" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="CT_attribute">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_attribute
  - Desc:  Defines the attributes and elements that make up a managed object
  -        attribute.
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="valueBLOB">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:base64Binary">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueBoolean">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="ST_booleanOrEmpty">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueCLOB">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueCompoundReference">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="CT_id">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueDate">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="ST_longOrEmpty">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueDecimal">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="ST_doubleOrEmpty">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueFilePath">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueGUID">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="CT_id">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueInt">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="ST_intOrEmpty">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueNumerical">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="ST_doubleOrEmpty">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueString">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="valueTinyInt">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="ST_shortOrEmpty">
								<xs:attributeGroup ref="AG_valueAttributes"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string"/>
	</xs:complexType>
	<xs:attributeGroup name="AG_valueAttributes">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  AG_valueAttributes
  - Desc:  Defines common attribs all VCM valueXXX elements contain.
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:attribute name="isNull" type="xs:boolean" use="optional" default="false"/>
	</xs:attributeGroup>
	<xs:complexType name="CT_attributeDefinitionData">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_attributeDefinitionData
  - Desc:  Defines an attribute definition.
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string"/>
			<xs:element name="displayName" type="xs:string"/>
			<xs:element name="columnSource" type="xs:string"/>
