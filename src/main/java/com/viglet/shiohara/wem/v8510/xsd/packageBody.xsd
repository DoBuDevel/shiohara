				<xs:element name="adhocReferences" type="CT_adhocReferences" minOccurs="0" maxOccurs="1"/>
				<xs:element name="classification" type="CT_classification" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="acl" type="CT_acl" minOccurs="0"/>
            </xs:sequence>
