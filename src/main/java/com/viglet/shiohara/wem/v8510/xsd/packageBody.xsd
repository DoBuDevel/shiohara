			<xs:element name="language" type="xs:string" minOccurs="0"/>
			<xs:element name="ordering" type="xs:integer"/>
			<xs:element name="extensibleOrdinal" type="xs:integer"/>
			<xs:element name="dataTypeValue" type="xs:integer"/>
			<xs:element name="required" type="xs:boolean"/>
			<xs:element name="autoGen" type="xs:boolean"/>
			<xs:element name="workflowRequired" type="xs:boolean"/>
			<xs:element name="key" type="xs:boolean"/>
			<xs:element name="searchable" type="xs:boolean"/>
			<xs:element name="summary" type="xs:boolean"/>
			<xs:element name="visible" type="xs:boolean"/>
			<xs:element name="defaultLabel" type="xs:boolean"/>
			<xs:element name="orderBy" type="xs:boolean"/>
			<xs:element name="attributeTypeValue" type="xs:integer"/>
			<xs:element name="widgetName" type="xs:string" minOccurs="0"/>
			<xs:element name="widgetData" type="xs:string" minOccurs="0"/>
			<xs:element name="referencedAttributeDefinitionName" type="xs:string" minOccurs="0"/>
            <xs:element name="beanPropertyName" type="xs:string" minOccurs="0"/>
            <xs:element name="suppressBeanProperty" type="xs:boolean" minOccurs="0"/>
            <xs:element name="groupSplit" type="xs:boolean" minOccurs="0"/>
            <xs:element name="groupName" type="xs:string" minOccurs="0"/>
            <xs:element name="toolTip" type="xs:string" minOccurs="0"/>
            <xs:element name="filterable" type="xs:boolean" minOccurs="0"/>
            <xs:element name="sharable" type="xs:boolean" minOccurs="0"/>
            <xs:element name="translatable" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
		<xs:attributeGroup ref="AG_appSvcsAttributes"/>
	</xs:complexType>
	<xs:complexType name="CT_channelAssociation">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_channelAssociation
  - Desc:  Defines a content association to a channel.
  -        This tag can appear in <contentInstance> and <staticFile>
  -        XML fragments.  The representation of relationships
  -        between a Channel and content appearing in either object
  -        uses the same construct.
  -        Appearing within a <contentInstance> or <staticFile>, each
  -        <referencId> tag within the <contentAssociation> element contains
  -        the AppSvcs ID to one or more channels.
  ---------------------------------------------------------------------------
			]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="referenceId" type="CT_id" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CT_channelLocalizedDataEntries">
		<xs:annotation>
		    <xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_channelLocalizedDataEntries
  - Desc:  Defines localized data entries for a channel entity.
  ---------------------------------------------------------------------------
		      ]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="localizedData" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="locale" type="CT_id"/>
						<xs:element name="active" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
						<xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="furlName" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="descriptor" type="xs:string" minOccurs="0" maxOccurs="1"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CT_channelInactiveFormatAssociations">
		<xs:annotation>
		    <xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_channelInactiveFormatAssociations
  - Desc:  Defines Channel-InactiveFormat asociations for a channel entity.
  ---------------------------------------------------------------------------
		      ]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="inactiveFormatAssocData" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="formatId" type="CT_id"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CT_adhocReferences">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_adhocReferences
  - Desc:  Ad-hoc references associated with a ManagedObject.
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="adhocReference" minOccurs="0" maxOccurs="unbounded">
