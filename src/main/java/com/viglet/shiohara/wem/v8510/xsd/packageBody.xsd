	<xs:element name="configNode" type="configRootNodeType">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ===========================================================================
  - Name:  configNode
  - Desc:  Description of a node in the configuration space.
  ===========================================================================
			]]></xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="configNodeType" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type which represents a node in the configuration space.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="inclusion" type="inclusionType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents an inclusion relation of a config node to its parent node</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="variable" type="configVariableType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents a config variable within a config node.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="node" type="configChildNodeType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents a child node within a config node.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the name of the node within its parent, used for building up the node's config id (path).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="type" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the type of component represented by the node, if any.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="state" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the state of the node.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="U">
						<xs:annotation>
							<xs:documentation xml:lang="en">unchanged</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="N">
						<xs:annotation>
							<xs:documentation xml:lang="en">new</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="D">
						<xs:annotation>
							<xs:documentation xml:lang="en">deleted</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="NP">
						<xs:annotation>
							<xs:documentation xml:lang="en">new and pushed</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="DP">
						<xs:annotation>
							<xs:documentation xml:lang="en">deleted and pushed</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="processNode" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies whether the node is a process node.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="productNode" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies whether the node is a product node.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="productState" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the product state of a product node.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="U">
						<xs:annotation>
							<xs:documentation xml:lang="en">unchanged</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="C">
						<xs:annotation>
							<xs:documentation xml:lang="en">changed</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="P">
						<xs:annotation>
							<xs:documentation xml:lang="en">pushed</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="configRootNodeType" final="#all">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type which represents a placeholder for a node in the configuration space at which to perform import. May undergo path replacement to be resolved to a real config node.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="configNodeType">
				<xs:attribute name="path" type="xs:string" use="optional" default="/">
					<xs:annotation>
						<xs:documentation xml:lang="en">Specifies the config id (path) of the node, subject to instance name replacement.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="configChildNodeType" final="#all">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type which represents a child node in the configuration space.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:restriction base="configNodeType">
				<xs:sequence>
					<xs:element name="inclusion" type="inclusionType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="en">Represents an inclusion relation of a config node to its parent node</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="variable" type="configVariableType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="en">Represents a config variable within a config node.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="node" type="configChildNodeType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="en">Represents a child node within a config node.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="configVariableType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type which represents a configuration variable.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="runValue" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents the current value of the variable, used by running processes.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="configVariableValueType"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="configValue" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents the new value of a variable in the Changed state.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="configVariableValueType"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="revertValue" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents the last committed value of a variable in the Changed state.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="configVariableValueType"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="defaultValue" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents the default value of the variable.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="configVariableValueType"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="overridable" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Specifies whether the variable can be overridden when referenced.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="value" use="optional" default="true">
						<xs:annotation>
							<xs:documentation xml:lang="en">Specifies the boolean value of the flag.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="flags" type="configVariableFlagSetType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents a set of boolean properties of the variable.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:attribute name="name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the name of the variable.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="state" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the state of the variable.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="U">
						<xs:annotation>
							<xs:documentation xml:lang="en">unchanged</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="N">
						<xs:annotation>
							<xs:documentation xml:lang="en">new</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="D">
						<xs:annotation>
							<xs:documentation xml:lang="en">deleted</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="C">
						<xs:annotation>
							<xs:documentation xml:lang="en">changed</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="NP">
						<xs:annotation>
							<xs:documentation xml:lang="en">new and pushed</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="DP">
						<xs:annotation>
							<xs:documentation xml:lang="en">deleted and pushed</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="CP">
						<xs:annotation>
							<xs:documentation xml:lang="en">changed and pushed</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the type of the variable.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="ENUM">
						<xs:annotation>
							<xs:documentation xml:lang="en">enumerated type</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="typeInfo" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies additional type information needed for the type of the variable.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="configVariableValueType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type which represents a configuration variable value.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:complexType name="flagType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type which represents a single boolean flag in a flag set.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="id" type="xs:nonNegativeInteger" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the id (bit position) of the flag.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the name of the flag.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="value" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the boolean value of the flag.</xs:documentation>
			</xs:annotation>
			<!-- "use" should really be "required" but the parser being used isn't treating the subsequent restrictions properly -->
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="configVariableFlagSetType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type which represents various boolean flags associated with config variables.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="interpreter" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents the value of the Interpreter flag.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Type which represents the "Interpreter" flag on a config variable.</xs:documentation>
					</xs:annotation>
					<xs:complexContent>
						<xs:restriction base="flagType">
							<xs:attribute name="id" type="xs:nonNegativeInteger" use="optional" default="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the id (bit position) of the flag.</xs:documentation>
									<xs:appinfo>Must match com.vignette.config.client.data.ConfigVarFlag.INTERPRETER_VAL.</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="name" type="xs:string" default="Interpreter">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the name of the flag.</xs:documentation>
									<xs:appinfo>Must match com.vignette.config.client.data.ConfigVarFlag.INTERPRETER_STR.</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="value" type="xs:boolean" default="true"/>
						</xs:restriction>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="restartNeeded" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents the value of the RestartNeeded flag, which specifies whether any processes must restart when the variable changes.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Type which represents the "RestartNeeded" flag on a config variable.</xs:documentation>
					</xs:annotation>
					<xs:complexContent>
						<xs:restriction base="flagType">
							<xs:attribute name="id" type="xs:nonNegativeInteger" use="optional" default="1">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the id (bit position) of the flag.</xs:documentation>
									<xs:appinfo>Must match com.vignette.config.client.data.ConfigVarFlag.RESTARTNEEDED_VAL.</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="name" type="xs:string" default="RestartNeeded">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the name of the flag.</xs:documentation>
									<xs:appinfo>Must match com.vignette.config.client.data.ConfigVarFlag.RESTARTNEEDED_STR.</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="value" type="xs:boolean" default="true"/>
						</xs:restriction>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="encrypted" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents the value of the Encrypted flag, which specifies whether the value of the variable is encrypted.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Type which represents the "Encrypted" flag on a config variable.</xs:documentation>
					</xs:annotation>
					<xs:complexContent>
						<xs:restriction base="flagType">
							<xs:attribute name="id" type="xs:nonNegativeInteger" use="optional" default="2">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the id (bit position) of the flag.</xs:documentation>
									<xs:appinfo>Must match com.vignette.config.client.data.ConfigVarFlag.ENCRYPTED_VAL.</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="name" type="xs:string" default="Encrypted">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the name of the flag.</xs:documentation>
									<xs:appinfo>Must match com.vignette.config.client.data.ConfigVarFlag.ENCRYPTED_STR.</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="value" type="xs:boolean" default="true"/>
						</xs:restriction>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="readOnly" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Represents the value of the ReadOnly flag, which specifies whether the variable is read-only.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:annotation>
						<xs:documentation xml:lang="en">Type which represents the "ReadOnly" flag on a config variable.</xs:documentation>
					</xs:annotation>
					<xs:complexContent>
						<xs:restriction base="flagType">
							<xs:attribute name="id" type="xs:nonNegativeInteger" use="optional" default="3">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the id (bit position) of the flag.</xs:documentation>
									<xs:appinfo>Must match com.vignette.config.client.data.ConfigVarFlag.READ_VAL.</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="name" type="xs:string" default="ReadOnly">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the name of the flag.</xs:documentation>
									<xs:appinfo>Must match com.vignette.config.client.data.ConfigVarFlag.READ_STR.</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="value" type="xs:boolean" default="true"/>
						</xs:restriction>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:all>
		<xs:attribute name="bits" type="xs:hexBinary" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the values of the bits of the flag set, as a big-endian hexadecimal string.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="inclusionType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Type which represents the inclusion relation of a config node to its parent node.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="parentPath" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Specifies the parent path of the node</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="type" type="xs:nonNegativeInteger" use="required">
