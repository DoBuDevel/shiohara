					<xs:sequence>
						<xs:element name="referenceName" type="xs:string" minOccurs="1" maxOccurs="1"/>
						<xs:element name="referentId" type="CT_id" minOccurs="1" maxOccurs="1"/>
						<xs:element name="metadata" type="xs:string" minOccurs="1" maxOccurs="1"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CT_classification">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_classification
  - Desc:  Taxonomy classification associated to a ManagedObject.
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="path" type="xs:string" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CT_id" mixed="true">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_id
  - Desc:  Type for elements whose body MAY contain an ID (VCM or AppSvcs).
  -        All elements of this type can also contain the <getId> element
  -        instead of an ID to have an object's ID resolved during import.
  - Exam:  <valueString>Some user data that's not an ID at all.</valueString>
  -        <valueString><getId ref="111"/></valueString>
  -        <parentChannelId>
  -	         <getId objectName="mySite/Home/myChan" objectType="channel"/>
  -        </parentChannelId>
  ---------------------------------------------------------------------------
    		]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="getId" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="ref" type="xs:string" use="optional"/>
					<xs:attribute name="objectName" type="xs:string" use="optional"/>
					<xs:attribute name="objectType" type="xs:string" use="optional"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CT_key">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_key
  - Desc:  Defines a reference's key(s).
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="attributeDefinitionName" type="xs:string"/>
			<xs:element name="sequence" type="xs:integer"/>
		</xs:sequence>
		<xs:attributeGroup ref="AG_appSvcsAttributes"/>
	</xs:complexType>
	<xs:complexType name="CT_reference">
		<xs:annotation>
			<xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_reference
  - Desc:  Defines a reference.
  ---------------------------------------------------------------------------
  			]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="deletionPolicyValue" type="xs:integer"/>
			<xs:element name="demotionPolicyValue" type="xs:integer"/>
			<xs:element name="exportPolicyValue" type="xs:integer"/>
			<xs:element name="keyCount" type="xs:integer"/>
			<xs:element name="promotionPolicyValue" type="xs:integer"/>
			<xs:element name="referencedContentTypeId" type="CT_id"/>
			<xs:element name="referenceTypeValue" type="xs:integer"/>
			<xs:element name="snapshotPolicyValue" type="xs:integer"/>
			<xs:element name="key" type="CT_key" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attributeGroup ref="AG_appSvcsAttributes"/>
	</xs:complexType>
	<xs:complexType name="CT_relation">
