	<xs:complexType name="CT_siteLocalizedDataEntries">
		<xs:annotation>
		    <xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_siteLocalizedDataEntries
  - Desc:  Defines localized data entries for a site entity.
  ---------------------------------------------------------------------------
		      ]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="localizedData" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="locale" type="CT_id"/>
						<xs:element name="active" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
						<xs:element name="required" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
						<xs:element name="default" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
						<xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="furlName" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="descriptor" type="xs:string" minOccurs="0" maxOccurs="1"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CT_siteFormatAssociations">
		<xs:annotation>
		    <xs:documentation xml:lang="en"><![CDATA[
  ---------------------------------------------------------------------------
  - Name:  CT_siteFormatAssociations
  - Desc:  Defines Site-Format associations for a site entity.
  ---------------------------------------------------------------------------
		      ]]></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="siteFormatAssocData" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="formatId" type="CT_id"/>
						<xs:element name="active" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
						<xs:element name="default" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
