			<xs:element name="displayName" type="xs:string"/>
			<xs:element name="minimum" type="xs:integer"/>
			<xs:element name="maximum" type="xs:integer"/>
            <xs:element name="beanPropertyName" type="xs:string" minOccurs="0"/>
            <xs:element name="suppressBeanProperty" type="xs:boolean" minOccurs="0"/>
			<xs:element name="attributeDefinition" type="CT_attributeDefinitionData" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="reference" type="CT_reference" minOccurs="0" maxOccurs="unbounded"/>
